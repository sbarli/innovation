{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "@inno/backend#build": {
      "dependsOn": ["^@inno/utils#build", "^@inno/constants#build"]
    },
    "@inno/backend#dev": {
      "dependsOn": ["^@inno/utils#build", "^@inno/constants#build"]
    },
    "lint": {},
    "lint:eslint": {},
    "lint:fix": {},
    "test": {},
    "clean": {
      "cache": false
    }
  }
}
