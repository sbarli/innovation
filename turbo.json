{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "@inno/backend#build": {
      "dependsOn": [
        "^@inno/utils#build",
        "^@inno/constants#build"
      ]
    },
    "@inno/backend#dev": {
      "dependsOn": [
        "^@inno/utils#build",
        "^@inno/constants#build"
      ]
    },
    "@inno/constants#build": {},
    "@inno/gql#build": {},
    "@inno/mocks#build": {
      "dependsOn": [
        "^@inno/gql#build"
      ]
    },
    "@inno/native#dev": {
      "dependsOn": [
        "^@inno/constants#build",
        "^@inno/gql#build",
        "^@inno/mocks#build",
        "^@inno/utils#build"
      ]
    },
    "@inno/utils#build": {},
    "lint": {},
    "lint:eslint": {},
    "lint:fix": {},
    "test": {},
    "clean": {
      "cache": false
    }
  }
}