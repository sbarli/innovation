{
  "name": "@inno/native",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "doppler run -- expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "dev": "yarn start",
    "predev": "./scripts/predev.sh",
    "exp": "npx expo",
    "exp:doc": "npx expo-doctor@latest",
    "lint:types": "yarn tsc",
    "lint:eslint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:fix": "yarn lint:eslint:fix",
    "lint:errors": "yarn lint:eslint --quiet",
    "lint:files-only": "yarn lint:eslint | grep /apps/native",
    "build:ios:dev": "yarn sync-secrets:dev && eas build --platform ios --profile development",
    "build:ios:dev-client": "yarn sync-secrets:dev && eas build --platform ios --profile development-client",
    "sync-secrets:dev": "doppler run --mount ./.env -- eas env:push development --path ./.env",
    "postinstall": "cd ../.. && TURBO_UI=0 yarn build"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@expo/html-elements": "0.4.2",
    "@expo/webpack-config": "^19.0.0",
    "@gluestack-ui/accordion": "^1.0.14",
