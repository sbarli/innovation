    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-status-bar": "~2.0.1",
    "graphql": "^16.8.1",
    "lucide-react-native": "^0.503.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.48.2",
